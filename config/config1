server  {
    listen 8080;
    server_name WEB-SERVER;
    error_page ./www/error.html;
    index ./www/index.html;
    location /
    {

        root /goinfre/xxxx/webserv/www;
        index  index.html;
        autoindex off;
        method_allowed GET;
        exec_cgi /goinfre/xxxx/webserv/cgi_tester;
        extension_cgi .bla .php .pl;
    }
    location /directory
    {
        root /goinfre/xxxx/webserv/www/YoupiBanane;
        index youpi.bad_extension;
        autoindex off;
        method_allowed GET POST;
        exec_cgi /goinfre/xxxx/webserv/cgi_tester;
        extension_cgi .bla .php .pl;
    }
    location /put_test
    {
        root /goinfre/xxxx/webserv/www/put_test;
    }
    location /post_body
    {
        root /goinfre/xxxx/webserv/www/x.html;
        method_allowed POST;
        body_size 100;
    }
    location /auth
    {
        root /goinfre/xxxx/webserv/www;
        index  index.html;
    }
}

server {


    listen 8081;
    server_name Hello;
    index  ./www/index.html;
    error_page /goinfre/xxxx/webserv/www/error.html;

}
